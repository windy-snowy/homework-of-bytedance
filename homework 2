//swift

class student{
   var name: String
   var majar: String 
   var age: Int 
}

class timeStruct {
    var time: Int
    init(time: Int) {
        self.time = time
    }
}

//objective-c

@interface Student:NSObject {
   //实例变量
   NSString name;    
   NSString majar;
   int age;
}
@property(nonatomic, readwrite) float time;  // Property

@end


